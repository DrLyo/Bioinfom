Sub 循环语句()
Dim i, j, k As Integer
 For i = 2 To 924
    For j = 2 To 186
    If Sheet1.Cells(i, 1) = Sheet1.Cells(j, 8) Then
     For k = 1 To 20
     Dim a(k) As Workbook
          a(k) = Sheet1.Cells(j, 9)
          Next k
          Sheet1.Cells(i, 2) = Max(a(k))
     End If
      Next j
     Next i
     
     
End Sub
